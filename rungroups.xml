<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Blue Rental Car">
    <test name="Minor Regression Test Group">
        <groups>
            <run>
                <!--            GROUP NAME = minor-regression-group
